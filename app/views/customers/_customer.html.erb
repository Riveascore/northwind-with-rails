<div id="<%= dom_id customer %>">
  <p>
    <strong>First name:</strong>
    <%= customer.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= customer.last_name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= customer.email %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= customer.phone %>
  </p>

  <p>
    <strong>Address1:</strong>
    <%= customer.address1 %>
  </p>

  <p>
    <strong>Address2:</strong>
    <%= customer.address2 %>
  </p>

  <p>
    <strong>City:</strong>
    <%= customer.city %>
  </p>

  <p>
    <strong>State:</strong>
    <%= customer.state %>
  </p>

  <p>
    <strong>Postal code:</strong>
    <%= customer.postal_code %>
  </p>

  <p>
    <strong>Country:</strong>
    <%= customer.country %>
  </p>

</div>
